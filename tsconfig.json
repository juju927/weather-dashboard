{
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["src/**/*"]
}
